{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Account": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "provider": {
          "type": "string"
        },
        "providerAccountId": {
          "type": "string"
        },
        "refresh_token": {
          "type": [
            "string",
            "null"
          ]
        },
        "access_token": {
          "type": [
            "string",
            "null"
          ]
        },
        "expires_at": {
          "type": [
            "integer",
            "null"
          ]
        },
        "token_type": {
          "type": [
            "string",
            "null"
          ]
        },
        "scope": {
          "type": [
            "string",
            "null"
          ]
        },
        "id_token": {
          "type": [
            "string",
            "null"
          ]
        },
        "session_state": {
          "type": [
            "string",
            "null"
          ]
        },
        "oauth_token_secret": {
          "type": [
            "string",
            "null"
          ]
        },
        "oauth_token": {
          "type": [
            "string",
            "null"
          ]
        },
        "user": {
          "$ref": "#/definitions/User"
        }
      },
      "required": [
        "userId",
        "type",
        "provider",
        "providerAccountId"
      ]
    },
    "Session": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "sessionToken": {
          "type": "string"
        },
        "expires": {
          "type": "string",
          "format": "date-time"
        },
        "user": {
          "$ref": "#/definitions/User"
        }
      },
      "required": [
        "sessionToken",
        "userId",
        "expires"
      ]
    },
    "User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string",
          "default": ""
        },
        "email": {
          "type": "string",
          "default": ""
        },
        "password": {
          "type": [
            "string",
            "null"
          ]
        },
        "hashedToken": {
          "type": [
            "string",
            "null"
          ]
        },
        "roles": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/UserRole"
          }
        },
        "createdAt": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "emailVerified": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "accounts": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Account"
          }
        },
        "image": {
          "type": [
            "string",
            "null"
          ]
        },
        "externalId": {
          "type": [
            "string",
            "null"
          ]
        },
        "sessions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Session"
          }
        }
      },
      "required": [
        "updatedAt"
      ]
    },
    "VerificationToken": {
      "type": "object",
      "properties": {
        "identifier": {
          "type": "string"
        },
        "token": {
          "type": "string"
        },
        "expires": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "identifier",
        "token",
        "expires"
      ]
    },
    "Role": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "users": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/UserRole"
          }
        }
      },
      "required": [
        "name"
      ]
    },
    "UserRole": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "user": {
          "$ref": "#/definitions/User"
        },
        "role": {
          "$ref": "#/definitions/Role"
        }
      },
      "required": [
        "userId",
        "roleId"
      ]
    }
  },
  "type": "object",
  "properties": {
    "account": {
      "$ref": "#/definitions/Account"
    },
    "session": {
      "$ref": "#/definitions/Session"
    },
    "user": {
      "$ref": "#/definitions/User"
    },
    "verificationToken": {
      "$ref": "#/definitions/VerificationToken"
    },
    "role": {
      "$ref": "#/definitions/Role"
    },
    "userRole": {
      "$ref": "#/definitions/UserRole"
    }
  }
}